/* pages/index/index.wxss */
@import "../../app.wxss";


page{
  background-color: #F8F8F8;
  font-size: 18px;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
  overflow-y: hidden;
}
.page__hd {
  padding: 40px;
}
.page__bd {
  padding-bottom: 40px;
}
.page__bd_spacing {
  padding-left: 15px;
  padding-right: 15px;
}

.page__ft{
  padding-bottom: 10px;
  text-align: left;
}

.page__title {
  text-align: left;
  font-size: 20px;
  font-weight: 400;
}

.page__desc {
  margin-top: 5px;
  color: #888888;
  text-align: left;
  font-size: 17px;
}
.weui-cell_example:before{
  left:52px;
}
page{
  background-color: #FFFFFF;
  height: 100%;
  line-height: 1.6;
}

.weui-tabs-bar__wrp {
  border-bottom: 1px solid #eee;
  margin-top: 10px;
}

.weui-tabs-swiper {
  width: 100%;
}

.tab-content {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
  padding: 20rpx;
}

.weui-tabs-bar__title {
  margin: 0px 20px;
  font-size: 18px;
}

.tab-bar-title__selected {
  font-size: 25px;
  font-weight: bold;
}

.weui-tabs-swiper{
  height: 672px;
  overflow-y: auto;
}
.p-b100{
  padding-bottom: 100px;
}
